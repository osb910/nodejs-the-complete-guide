<!DOCTYPE html>
<html>
  <head>
    <%- include('components/head.ejs') %>
  </head>
  <body>
    <main>
      <ul>
        <% if (users.length === 0) { %>
        <h1>No Users Found!</h1>
        <% } else { %>
        <h1>Users</h1>
        <% users.forEach(user => { %>
        <li class="user"><%= user.name %></li>
        <% })} %>
      </ul>
    </main>
  </body>
</html>
